{
    "name": "Share2misskey.io",
    "description": "Share to page title and URL for https://misskey.io",
    "version": "0.2",
    "manifest_version": 3,
    "author": "betarium",
    "homepage_url": "https://github.com/betarium/share2misskey-io",
    "icons": {
        "16": "icons/icon16.png",
        "19": "icons/icon19.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}